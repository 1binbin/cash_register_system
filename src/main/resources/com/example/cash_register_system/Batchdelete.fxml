<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.cash_register_system.Adminevent">
    <children>
        <VBox alignment="CENTER" fillWidth="false" layoutX="235.0" layoutY="48.0" spacing="10.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <Label text="温馨提示：请用 Tab 键隔开每一个编号" textFill="RED">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextArea fx:id="badeleteta" prefHeight="271.0" prefWidth="592.0" wrapText="true">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </TextArea>
                <HBox alignment="CENTER" prefHeight="58.0" prefWidth="600.0" spacing="100.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#resetbatchdelete" text="重新选择">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#getbatchvalue" text="确定选择">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#deleteok" text="确定删除">
                            <font>
                                <Font size="17.0"/>
                            </font>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
